{
  "extends": "./tsconfig.base.json",
  "include": ["src"],
  "compilerOptions": {
    "strict": true,
    "exactOptionalPropertyTypes": true,
    "types": ["node"],
    "tsBuildInfoFile": ".tsbuildinfo/src.tsbuildinfo",
    "rootDir": "src",
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
